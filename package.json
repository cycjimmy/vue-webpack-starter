{
  "name": "vue-webpack-starter",
  "version": "2.0.1",
  "description": "vue webpack starter",
  "scripts": {
    "start": "npm run dev",
    "lint": "eslint -c .eslintrc .",
    "lint:fix": "eslint --fix -c .eslintrc .",
    "dev": "npx webpack-cli --node-env development -c webpack/webpack.dev.config.js -w --color",
    "build": "npx webpack-cli --node-env production -c webpack/webpack.build.config.js --progress --color",
    "build:watch": "npx webpack-cli --node-env production -c webpack/webpack.build.config.js --progress --color -w",
    "build:test_server:watch": "npx webpack-cli --node-env production_test_server -c webpack/webpack.build.config.js --progress --color -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycjimmy/vue-webpack-starter.git"
  },
  "keywords": [
    "vue",
    "webpack"
  ],
  "author": "cycjimmy <cycjimmy@gmail.com> (https://github.com/cycjimmy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cycjimmy/vue-webpack-starter/issues"
  },
  "homepage": "https://github.com/cycjimmy/vue-webpack-starter#readme",
  "dependencies": {
    "@cycjimmy/awesome-js-funcs": "^4.0.6",
    "@cycjimmy/mini-xhr": "^5.0.0",
    "@cycjimmy/sass-lib": "^2.0.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@cycjimmy/config-lib": "^3.0.5",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.9",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.1.4",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.11"
  }
}
